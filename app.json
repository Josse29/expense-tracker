{
  "expo": {
    "name": "expense-tracker",
    "slug": "expense-tracker",
    "scheme": "expense-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/img/logo.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/img/logo.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#315EFF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/img/logo.jpg",
        "backgroundColor": "#315EFF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jossedev.expensetracker"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./src/assets/img/logo.jpg"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8ed34672-b7ad-48fb-8f92-c0aa9b6e6b12"
      }
    }
  }
}
